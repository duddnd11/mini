<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.yw.mapper.MemberMapper">
 	<insert id="insertMember">
 		INSERT INTO member(id,name,password,enabled,email,gender,birth,phoneNum) VALUES(#{id},#{name},#{password},1,#{email},#{gender},#{birth},#{phoneNum});	
 	</insert>
 	
 	<select id="getUserById" parameterType="String" resultType="com.yw.security.CustomUserDetail">
 		SELECT * FROM member WHERE id=#{id};
 	</select>
 </mapper>